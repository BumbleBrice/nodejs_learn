<%- include('header') -%>
<main>
    <h2>Liste des contacts</h2>
    <form action="/supContact" method="post">
        <ul>
            <% for(let i=0;i<contacts.length;i++){ %>
                <li><input type="checkbox" name="sup[]" value="<%= i %>"> <span class="contact"><%= contacts[i][1].toUpperCase() %> <%= contacts[i][2] %> </span><a href="/details/<%= i %>"><i class="far fa-eye"></i></a></li>
            <% } %>
        </ul>
        <button>Supprimer</button>
    </form>
</main>
<%- include('footer') -%>